{
  "template": "<div class=\"check\"><form @submit.prevent=\"submitForm\"><div v-for=\"(field, index) in formTemplate.fields\" :key=\"index\"><label :for=\"field.label\">{{ field.label }}</label><input :type=\"field.type\" :placeholder=\"field.placeholder\" v-model=\"formData[field.label]\" @input=\"validateInput(field.label)\" /><div v-if=\"validationErrors[field.label]\">{{ validationErrors[field.label] }}</div></div><button type=\"submit\">Submit</button></form></div>"
}
